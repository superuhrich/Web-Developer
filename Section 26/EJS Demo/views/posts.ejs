<%- include('partials/header')%> 



<link rel="stylesheet" href="app.css">

<h1>The Post page</h1>

<% for(var i =0; i< posts.length; i++){ %>
<li><%= posts[i].title %> - <strong><%= posts[i].author %></strong> </li>
<% } %>

<% posts.forEach(post => { %>
<li>
    <%= post.title %> - <%= post.author %>
</li>
<%  });  %>

